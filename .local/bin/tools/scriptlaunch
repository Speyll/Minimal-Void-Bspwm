#!/bin/sh
choice=$(echo "dmenumount\ndmenuumount\nducksearch\nrecordmenu\nrecordkill" | dmenu -i -p "Choose what script launch:")

case "$choice" in
    dmenumount) dmenumount & ;;
    dmenuumount) dmenuumount & ;;
    ducksearch) ducksearch & ;;
    recordmenu) recordmenu & ;;
    recordkill) recordkill & ;;
esac
