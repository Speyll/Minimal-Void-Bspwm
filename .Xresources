! font settings
*.font: -kakwa-kakwafont-medium-r-normal--12-120-72-72-c-60-iso8859-1,\
        -wuncon-siji-medium-r-normal--10-100-75-75-c-80-iso10646-1
*.boldFont: -kakwa-kakwafont-medium-r-bold--12-120-72-72-c-60-iso8859-1
*.italicFont: -kakwa-kakwafont-medium-r-normal--12-120-72-72-c-60-iso8859-1
*.boldItalicFont: -kakwa-kakwafont-medium-r-bold--12-120-72-72-c-60-iso8859-1

Xft.hinting: true
Xft.autohint: true
Xft.rgba: true
Xft.lcdfilter: lcddefault

! urxvt settings
urxvt.termName: rxvt-unicode-256color
urxvt.scrollBar: false
urxvt.scrollbar_right: false
urxvt.internalBorder: 25
urxvt.cursorBlink: true

! colors
*.foreground:	#657b83
*.background:	#002b36
*.cursorColor:	#657b83
!*.fading:	40
*.fadeColor:	#002b36
*.cursorColor:	#93a1a1
*.pointerColorBackground:	#586e75
*.pointerColorForeground:	#93a1a1

! black
*.color0:       #073642
*.color8:       #002b36

! red
*.color1:       #dc322f
*.color9:       #cb4b16

! green
*.color2:       #859900
*.color10:      #586e75

! yellow
*.color3:       #b58900
*.color11:      #657b83

! blue
*.color4:       #268bd2
*.color12:      #839496

! magenta
*.color5:       #d33682
*.color13:      #6c71c4

! cyan
*.color6:       #2aa198
*.color14:      #93a1a1

! white
*.color7:       #eee8d5
*.color15:      #fdf6e3
