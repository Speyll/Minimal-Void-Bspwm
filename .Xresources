! font settings
*.font: xft:terminus:style=Regular:size=9
*.boldFont: xft:terminus:style=Regular:size=9
*.italicFont: xft:terminus:style=Regular:size=9
*.boldItalicFont: xft:terminus:style=Regular:size=9

Xft.hinting: true
Xft.rgba: false
Xft.autohint: true
Xft.hintstyle: none
Xft.lcdfilter: lcddefault

! urxvt settings
URxvt.termName: rxvt-unicode-256color
URxvt.scrollBar: false
URxvt.scrollbar_right: false
URxvt.internalBorder: 25
URxvt.depth: 32
URxvt.cursorBlink: true

!fake transparency
URxvt.inheritPixmap: true
URxvt.transparent: true
URxvt.shading: 35

/*
!true transparency
URxvt.background: [80]#212133
*/


! custom nord
*.background: #161623
*.foreground: #a6bbd9

! black
*.color0: #101010
